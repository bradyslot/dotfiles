#!/bin/sh

fc-cache -f &
xsettingsd &
xrandr -r 75 &
clipmenud &
uncluttler -idle 0 &
xcalib ~/.config/xcalib/q3279vwf.icm &
xset s off -dpms 2> /dev/null &
xset r rate 350 40 &
xmodmap -e 'clear Lock' -e 'keycode 0x42 = Escape' &
xrdb ~/.config/X11/Xresources &
feh --no-fehbg --bg-fill ~/Pictures/motoko-kusanagi-0-wallpaper.png &
dwmblocks &
picom --no-fading-openclose -m 1.0 &
sxhkd &
dunst &

while true; do
	dbus-launch dwm 2> /dev/null
done
