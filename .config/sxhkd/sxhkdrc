@XF86Audio{RaiseVolume,LowerVolume,Mute,MicMute}
    pactl -- {set-sink-volume 0 +5%,set-sink-volume 0 -5%,set-sink-mute 0 toggle,set-source-mute 0 toggle}

@XF86MonBrightness{Up,Down}
    sudo xbacklight {+10,-10}

Print
	scrot Pictures/scrot/screenshot-$(date '+%F-%H-%M-%S').png

alt + Print
	scrot -sf Pictures/scrot/screengrab-$(date '+%F-%H-%M-%S').png

alt + backslash
	clipcat-menu
